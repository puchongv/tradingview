        array.push(sell_entry_times_60, time)

// 10min Winrate Calculation
if enable_10min and array.size(buy_entry_prices_10) > 0
    for i = array.size(buy_entry_prices_10) - 1 to 0
        entry_time = array.get(buy_entry_times_10, i)
