plot(enable_30min ? total_sell_signals_30 : na, title="ğŸ“‰ Sell Signals 30min", display=display.data_window)
plot(enable_30min ? total_wins_30 : na, title="âœ… Wins 30min", display=display.data_window)
plot(enable_30min ? total_losses_30 : na, title="âŒ Losses 30min", display=display.data_window)
plot(enable_30min ? win_rate_30 : na, title="ğŸ“Š Win Rate 30min %", display=display.data_window)

// Debug: 60min Winrate Data
plot(enable_60min ? total_buy_signals_60 : na, title="ğŸ“ˆ Buy Signals 60min", display=display.data_window)
plot(enable_60min ? total_sell_signals_60 : na, title="ğŸ“‰ Sell Signals 60min", display=display.data_window)
